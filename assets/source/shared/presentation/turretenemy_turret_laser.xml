			<translation x="1.2"/>
		</frameanim>
		<frameanim directory="spritestrip/turretenemy/gun/guns" lightmask="use_lightmask" quad_width="1.867" quad_height="1.215" fps="10" looping="true">
			<begin frame="0"/>
			<end   frame="0"/>
			<translation x="-1.2"/>
			<rotation value="180"/>
		</frameanim>
	</frameanimstack>

	<!--barrelcount_4-->
	<frameanimstack>
		<musthave>
			<tag name="barrelcount_4"/>
		</musthave>
		<mustnothave>
			<tag name="ishackable"/>
		</mustnothave>
