<?xml version="1.0" encoding="utf-8"?>
<presentation xmlns="http://intranet.twotribes.com/schemas/common/presentation.xsd"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://intranet.twotribes.com/schemas/common/presentation.xsd http://intranet.twotribes.com/schemas/common/presentation.xsd"
		permanent="true" fixed_timestep="0.016">

	<startvalues>
		<translation z="0.4"/>
	</startvalues>

	<frameanimstack>
		<frameanim texture_anim_v="10" filter_mipmap="none" flip_horizontal="false" flip_vertical="true" frame_width="1" frame_height="25" blendmode="blend" image="scanlines" image_namespace="textures.hud" fps="1" looping="true" quad_width="2" quad_height="1">
			<begin frame="0" hold="true"/>
			<end   frame="0" hold="true"/>
		</frameanim>
	</frameanimstack>

	<frameanimstack>
		<musthave>
			<tag name="dead"/>
		</musthave>
		<frameanim texture_anim_u="12.392" texture_anim_v="0.17" flip_horizontal="false" flip_vertical="true" frame_width="3" frame_height="1" blendmode="add" image="static" filter_magnify="point" image_namespace="textures.hud" fps="1" looping="true" quad_width="7.2" quad_height="1.2">
			<begin frame="0" hold="true"/>
			<end   frame="0" hold="true"/>
			<translation x="0" y="0"/>
		</frameanim>
	</frameanimstack>

</presentation>
