<?xml version="1.0" encoding="utf-8"?>
<presentation xmlns="http://intranet.twotribes.com/schemas/common/presentation.xsd"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://intranet.twotribes.com/schemas/common/presentation.xsd http://intranet.twotribes.com/schemas/common/presentation.xsd"
		dont_precache="true" permanent="true">

	<startvalues>
		<scale scale="0.99259"/>
	</startvalues>

	<frameanimstack>
		<frameanim directory="spritestrip/loading/logo" fps="30" quad_width="1.92" quad_height="1.08">
			<begin frame="0" hold="true"/>
			<end   frame="0" hold="true"/>
			<translation x="0.0" y="0.0"/>
		</frameanim>
	</frameanimstack>

	<particles file="particles/logo_rive_electricity_loading.trigger" position_type="follow">
		<translation x="-0.1395" y="0.18" z="0"/>
		<scale scale="0.085"/>
	</particles>
	
	<particles file="particles/dust_loading.trigger" position_type="follow">
		<translation x="-0.5" y="0.0" z="0.2"/>
		<scale scale="0.085"/>
	</particles>

	<trigger data="menu_load_loop" type="global_audio"/>

</presentation>
