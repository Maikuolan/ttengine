<?xml version="1.0" encoding="utf-8"?>
<presentation xmlns="http://intranet.twotribes.com/schemas/common/presentation.xsd"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://intranet.twotribes.com/schemas/common/presentation.xsd http://intranet.twotribes.com/schemas/common/presentation.xsd"
		permanent="true">

	<!--0-->
	<startvalues>
		<musthave>
			<tag name="0"/>
		</musthave>
		<scale scale="0.75"/>
		<scale scale="c(scale)"/>
		<origin x="1.1" y="0.8"/>
		<translation x="0.0" y="-0.9"/>
	</startvalues>

	<!--90-->
	<startvalues>
		<musthave>
			<tag name="90"/>
		</musthave>
		<scale scale="0.75"/>
		<scale scale="c(scale)"/>
		<origin x="0.8" y="-1.1"/>
		<translation x="-0.9" y="1.2"/>
	</startvalues>

	<!--180-->
	<startvalues>
		<musthave>
			<tag name="180"/>
		</musthave>
		<scale scale="0.75"/>
		<scale scale="c(scale)"/>
		<origin x="-1.1" y="0.8"/>
		<translation x="0" y="-0.9"/>
	</startvalues>

	<!--270-->
	<startvalues>
		<musthave>
			<tag name="270"/>
		</musthave>
		<scale scale="0.75"/>
		<scale scale="c(scale)"/>
		<origin x="0.8" y="1.1"/>
		<translation x="-0.9" y="1.2"/>
	</startvalues>

	<!--mainbox front-->
	<!--0-->
	<frameanimstack>
		<musthave>
			<tag name="0"/>
		</musthave>
		<frameanim directory="spritestrip/pipegate/mainbox_0" lightmask="use_lightmask" fps="30" quad_width="4.297" quad_height="8" looping="true">
			<begin frame="0" hold="true"/>
			<end   frame="0" hold="true"/>
		</frameanim>
	</frameanimstack>

	<!--90-->
	<frameanimstack>
		<musthave>
			<tag name="90"/>
		</musthave>
		<frameanim directory="spritestrip/pipegate/mainbox_90" lightmask="use_lightmask" fps="30" quad_width="8" quad_height="4.297" looping="true">
			<begin frame="0" hold="true"/>
			<end   frame="0" hold="true"/>
		</frameanim>
	</frameanimstack>

	<!--180-->
	<frameanimstack>
			<musthave>
				<tag name="180"/>
			</musthave>
		<frameanim directory="spritestrip/pipegate/mainbox_0" flip_horizontal="true" lightmask="use_lightmask" fps="30" quad_width="4.297" quad_height="8" looping="true">
			<begin frame="0" hold="true"/>
			<end   frame="0" hold="true"/>
			
		</frameanim>
	</frameanimstack>

	<!--270-->
	<frameanimstack>
		<musthave>
			<tag name="270"/>
		</musthave>
		<frameanim directory="spritestrip/pipegate/mainbox_270" lightmask="use_lightmask" fps="30" quad_width="8" quad_height="4.297" looping="true">
			<begin frame="0" hold="true"/>
			<end   frame="0" hold="true"/>

		</frameanim>
	</frameanimstack>
	
	

	<!--light green-->
	<!--0-->
	<frameanimstack name="open">
		<musthave>
			<tag name="0"/>
		</musthave>
		<frameanim directory="spritestrip/pipegate/lights" lightmask="use_alpha" fps="30" quad_width="2" quad_height="2" looping="true">
			<begin frame="0" hold="true"/>
			<end   frame="0" hold="true"/>
			<translation x="-1.4" y="-0.5"/>
		</frameanim>
	</frameanimstack>
	
	<trigger name="open" type="audio" data="pipegate_open"/>

	<!--90-->
	<frameanimstack name="open">
		<musthave>
			<tag name="90"/>
		</musthave>
		<frameanim directory="spritestrip/pipegate/lights" lightmask="use_alpha" fps="30" quad_width="2" quad_height="2" looping="true">
			<begin frame="0" hold="true"/>
			<end   frame="0" hold="true"/>
			<translation x="-0.5" y="1.4"/>
		</frameanim>
	</frameanimstack>

	<!--180-->
	<frameanimstack name="open">
		<musthave>
			<tag name="180"/>
		</musthave>
		<frameanim directory="spritestrip/pipegate/lights" flip_horizontal="true" lightmask="use_alpha" fps="30" quad_width="2" quad_height="2" looping="true">
			<begin frame="0" hold="true"/>
			<end   frame="0" hold="true"/>
			<translation x="1.4" y="-0.5"/>
		</frameanim>
	</frameanimstack>

	<!--270-->
	<frameanimstack name="open">
		<musthave>
			<tag name="270"/>
		</musthave>
		<frameanim directory="spritestrip/pipegate/lights" lightmask="use_alpha" fps="30" quad_width="2" quad_height="2" looping="true">
			<begin frame="0" hold="true"/>
			<end   frame="0" hold="true"/>
			<translation x="-0.5" y="-1.4"/>
		</frameanim>
	</frameanimstack>

	<!--light red-->
	<!--0-->
	<frameanimstack name="close">
		<musthave>
			<tag name="0"/>
		</musthave>
		<frameanim directory="spritestrip/pipegate/lights" lightmask="use_alpha" fps="30" quad_width="2.3" quad_height="2.3" looping="true">
			<begin frame="1" hold="true"/>
			<end   frame="1" hold="true"/>
			<translation x="-1.4" y="0.6"/>
		</frameanim>
	</frameanimstack>

	<trigger name="close" type="audio" data="pipegate_close"/>

	<!--90-->
	<frameanimstack name="close">
		<musthave>
			<tag name="90"/>
		</musthave>
		<frameanim directory="spritestrip/pipegate/lights" lightmask="use_alpha" fps="30" quad_width="2.3" quad_height="2.3" looping="true">
			<begin frame="1" hold="true"/>
			<end   frame="1" hold="true"/>
			<translation x="0.6" y="1.4"/>
		</frameanim>
	</frameanimstack>

	<!--180-->
	<frameanimstack name="close">
		<musthave>
			<tag name="180"/>
		</musthave>
		<frameanim directory="spritestrip/pipegate/lights" lightmask="use_alpha" fps="30" quad_width="2.3" quad_height="2.3" looping="true">
			<begin frame="1" hold="true"/>
			<end   frame="1" hold="true"/>
			<translation x="1.4" y="0.6"/>
		</frameanim>
	</frameanimstack>

	<!--270-->
	<frameanimstack name="close">
		<musthave>
			<tag name="270"/>
		</musthave>
		<frameanim directory="spritestrip/pipegate/lights" lightmask="use_alpha" fps="30" quad_width="2.3" quad_height="2.3" looping="true">
			<begin frame="1" hold="true"/>
			<end   frame="1" hold="true"/>
			<translation x="0.6" y="-1.4"/>
		</frameanim>
	</frameanimstack>
	
	<!--light idle-->
	<!--0-->
	<frameanimstack name="idle">
		<musthave>
			<tag name="0"/>
		</musthave>
		<frameanim directory="spritestrip/pipegate/lights" lightmask="use_alpha" fps="30" quad_width="2.3" quad_height="2.3" looping="true">
			<begin frame="1" hold="true"/>
			<end   frame="1" hold="true"/>
			<translation x="-1.4" y="0.6"/>
		</frameanim>
	</frameanimstack>

	<!--90-->
	<frameanimstack name="idle">
		<musthave>
			<tag name="90"/>
		</musthave>
		<frameanim directory="spritestrip/pipegate/lights" lightmask="use_alpha" fps="30" quad_width="2.3" quad_height="2.3" looping="true">
			<begin frame="1" hold="true"/>
			<end   frame="1" hold="true"/>
			<translation x="0.6" y="1.4"/>
		</frameanim>
	</frameanimstack>

	<!--180-->
	<frameanimstack name="idle">
		<musthave>
			<tag name="180"/>
		</musthave>
		<frameanim directory="spritestrip/pipegate/lights" lightmask="use_alpha" fps="30" quad_width="2.3" quad_height="2.3" looping="true">
			<begin frame="1" hold="true"/>
			<end   frame="1" hold="true"/>
			<translation x="1.4" y="0.6"/>
		</frameanim>
	</frameanimstack>

	<!--270-->
	<frameanimstack name="idle">
		<musthave>
			<tag name="270"/>
		</musthave>
		<frameanim directory="spritestrip/pipegate/lights" lightmask="use_alpha" fps="30" quad_width="2.3" quad_height="2.3" looping="true">
			<begin frame="1" hold="true"/>
			<end   frame="1" hold="true"/>
			<translation x="0.6" y="-1.4"/>
		</frameanim>
	</frameanimstack>
</presentation>
