		xsi:schemaLocation="http://intranet.twotribes.com/schemas/common/presentation.xsd http://intranet.twotribes.com/schemas/common/presentation.xsd"
		dont_precache="true" permanent="true">

	<startvalues>
		<scale y="1.1" x="1"/>
		<origin x="0.55"/>
	</startvalues>

	<!--bottom block-->

	<frameanimstack>
		<frameanim directory="spritestrip/pistons/pistoncover/horizontal" lightmask="use_lightmask" fps="15" looping="false" quad_width="1" quad_height="1.074" direction="forward">
			<begin frame="0" hold="true"/>
			<end   frame="0" hold="true"/>
		</frameanim>
	</frameanimstack>
