<?xml version="1.0" encoding="utf-8"?>
<presentation xmlns="http://intranet.twotribes.com/schemas/common/presentation.xsd"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://intranet.twotribes.com/schemas/common/presentation.xsd http://intranet.twotribes.com/schemas/common/presentation.xsd"
		permanent="true">

	<startvalues>
		<translation x="0.05"/>
	</startvalues>
  
	<!--FOREGROUND-->
	<frameanimstack>
		<frameanim directory="spritestrip/hud/collectibles" fps="1" quad_width="0.06" quad_height="0.06" direction="forward">
			<begin frame="0" hold="true"/>
			<end   frame="0" hold="true"/>
			<translation x="-0.085"/>
		</frameanim>
	</frameanimstack>  

	
	<coloranimstack>
		<color duration="0.01">
			<begin r="0" g="0" b="0" a="-130"/>
			<end   r="0" g="-0" b="-0" a="-130"/>
		</color>
	</coloranimstack>
	
</presentation>